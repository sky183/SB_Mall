<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.sb.mall.dao.MemberDao">
 	<insert id="insert"
 		parameterType="com.sb.mall.model.MemberInfo"
 		useGeneratedKeys="true" keyColumn="userSeq">
 		insert into member (userId,userPw,userName,address1,address2,regDate)
 		values (#{userId},#{userPw},#{userName},#{address1},#{address2},now())
 	</insert>
 </mapper>