<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sb.mall.dao.OrderDao">
	<insert id="insertCart" parameterType="com.sb.mall.model.Order"
		useGeneratedKeys="true" keyProperty="orderSeq">
		insert into
		`Order`(orderDetailNum,userSeq,productSeq,quantity,`option`)
		value(0,#{userSeq},#{productSeq},#{quantity},#{option})
	</insert>

	<update id="update" parameterType="com.sb.mall.model.Order">
	</update>

	<update id="updateStatus" parameterType="Map">
	</update>

	<delete id="delete" parameterType="int">
	</delete>

	<select id="selectAll" resultType="com.sb.mall.model.Order">
	</select>

	<select id="selectColumn" resultType="com.sb.mall.model.Order"
		parameterType="Map">
	</select>

	<select id="selectJoin" resultType="com.sb.mall.model.Order"
		parameterType="Map">
	</select>

	<select id="selectMemberOrder"
		resultType="com.sb.mall.model.Order">
	</select>

	<select id="select" resultType="com.sb.mall.model.Order"
		parameterType="String">
		select * from `Order` where orderDetailNum = #{orderDetailNum}
	</select>

	<select id="selectOrder" resultType="com.sb.mall.model.Order">
	</select>




</mapper>